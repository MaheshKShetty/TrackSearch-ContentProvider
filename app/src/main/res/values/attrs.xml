<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSearchView">
        <attr name="searchBackground" format="integer" />
        <attr name="searchVoiceIcon" format="integer" />
        <attr name="searchCloseIcon" format="integer" />
        <attr name="searchBackIcon" format="integer" />
        <attr name="searchSuggestionBackground" format="integer" />
        <attr name="historyIcon" format="integer" />
        <attr name="suggestionIcon" format="integer" />
        <attr name="listTextColor" format="integer" />
        <attr name="searchBarHeight" format="dimension" />
        <attr name="voiceHintPrompt" format="string" />
        <attr name="voiceIconEnabled" format="boolean" />
        <attr name="android:fitsSystemWindows" />
        <attr name="android:hint" />
        <attr name="android:textColor" />
        <attr name="android:textColorHint" />
        <attr name="android:inputType" />
    </declare-styleable>
    <declare-styleable name="OtpEditTextClass">
        <attr name="editTextBackgroundStyle" format="enum">
            <enum name="box" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>
</resources>